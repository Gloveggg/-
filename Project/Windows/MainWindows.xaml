<Window x:Class="Project.Windows.MainWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.Windows"
        mc:Ignorable="d"
        Title="MainWindows" Height="900" Width="1600">
    <Grid Background="AliceBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="9*" />
        </Grid.RowDefinitions>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Button Name="Record" Content="Запись" Click="Record_Click" />
            <Button Name="Klients" Content="Клиенты" Click="Klients_Click" Grid.Column="1"/>
            <Button Name="Service" Content="Услуги" Click="Service_Click" Grid.Column="2"/>
            <Button Name="Materials" Content="Материалы" Click="Materials_Click"  Grid.Column="3"/>
            <Button Name="Barber" Content="Барбер" Click="Barber_Click" Grid.Column="4"/>

        </Grid>
        <Frame Name="MainFrm" Grid.Row="1" Grid.ColumnSpan="4"
               NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
