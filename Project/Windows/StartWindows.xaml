<Window x:Class="Project.Windows.StartWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.Windows"
        mc:Ignorable="d"
        Title="StartWindows" Height="350" Width="400">
    <Grid>
        <StackPanel Background="AliceBlue">
            <TextBlock Text="Login" FontSize="20"/>
            <TextBox x:Name="LoginTb" Background="AliceBlue"/>
            <TextBlock Text="Password" Background="AliceBlue" FontSize="20"/>
            <PasswordBox x:Name="PasswordTb" Background="AliceBlue"/>
            <Button Content="Войти" Background="AliceBlue" Height="30" Click="Button_Click"/>
           

        </StackPanel>
        <Frame Name="StartFrm"
               
               NavigationUIVisibility="Hidden" Navigated="StartFrm_Navigated"/>
    </Grid>
</Window>
